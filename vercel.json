{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/", "dest": "/api/index.js" },
    { "src": "/login", "dest": "/api/index.js" },
    { "src": "/register", "dest": "/api/index.js" },
    { "src": "/dashboard", "dest": "/api/index.js" },
    { "src": "/chat", "dest": "/api/index.js" },
    { "src": "/workouts", "dest": "/api/index.js" },
    { "src": "/progress", "dest": "/api/index.js" },
    { "src": "/nutrition", "dest": "/api/index.js" },
    { "src": "/nutriscan", "dest": "/api/index.js" },
    { "src": "/meal-planner", "dest": "/api/index.js" },
    { "src": "/challenges", "dest": "/api/index.js" },
    { "src": "/health", "dest": "/api/index.js" },
    { "src": "/biometrics", "dest": "/api/index.js" },
    { "src": "/schedule", "dest": "/api/index.js" },
    { "src": "/community", "dest": "/api/index.js" },
    { "src": "/ai-coach", "dest": "/api/index.js" },
    { "src": "/settings", "dest": "/api/index.js" },
    { "src": "/CustomOnboarding", "dest": "/api/index.js" },
    { "src": "/forgot-password", "dest": "/api/index.js" },
    { "src": "/reset-password", "dest": "/api/index.js" },
    { "src": "/logout", "dest": "/api/index.js" },
    { "src": "/signup", "dest": "/api/index.js" },
    { "src": "/send", "dest": "/api/index.js" },
    { "src": "/friend-requests", "dest": "/api/index.js" },
    { "src": "/css/(.*)", "dest": "/api/index.js" },
    { "src": "/js/(.*)", "dest": "/api/index.js" },
    { "src": "/images/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/api/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}