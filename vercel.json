{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "views/**/*.ejs",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/CustomOnboarding/complete",
      "dest": "/app.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ]
}